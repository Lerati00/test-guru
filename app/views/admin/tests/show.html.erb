<p>
  <strong>Title:</strong>
  <%= @test.title %>
</p>

<p>
  <strong>Level:</strong>
  <%= test_level(@test) %>
</p>

<p>
  <table>
    <tr>
      <th>Questions <%= @test.title %> </th>
      <th colspan='3'>Actions</th>
    </tr>
    <%= render @test.questions %>
    <tr>
      <td colspan='4'> <%= link_to "Create new Question", new_admin_test_question_path(@test, @question) %> </td>
    </tr>
  </table>
</p>

<p>
<%= link_to "Back", admin_tests_path %> |
<td> <%= link_to "Edit", edit_admin_test_path(@test) %> </td> 
</p>

