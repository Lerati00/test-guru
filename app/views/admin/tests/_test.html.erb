<tr>
  <td>
    <%= content_tag :span, test.title, class: 'test-title', data: { test_id: test.id } %>
    <%= render 'form_inline', test: test == @test ? @test : test %> 
  </td>
  <td> <%= test_level(test) %> </td>
  <td> <%= test.questions.size %> </td>
  <td> <%= link_to t('admin.control_link.show'), admin_test_path(test), class: "btn btn-success"%> </td>
  <td> 
    <%= link_to t('admin.control_link.edit'), 
                edit_admin_test_path(test),
                data: { test_id: test.id }, 
                class: "btn btn-warning form-inline-link" %> 
  </td>
  <td> 
    <%= link_to t('admin.control_link.delete'), 
                admin_test_path(test), 
                method: :delete,
                class: "btn btn-danger",
                data: {confirm: t('.confirm') } %> 
  </td>
</tr>
