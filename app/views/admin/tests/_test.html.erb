<tr>
  <td> <%= test.title %> </td>
  <td> <%= test_level(test) %> </td>
  <td> <%= test.questions.size %> </td>
  <td> <%= link_to t('admin.control_link.show'), admin_test_path(test), class: "btn btn-success"%> </td>
  <td> <%= link_to t('admin.control_link.edit'), edit_admin_test_path(test), class: "btn btn-warning" %> </td>
  <td> 
    <%= link_to t('admin.control_link.delete'), 
                admin_test_path(test), 
                method: :delete,
                class: "btn btn-danger",
                data: {confirm: t('.confirm') } %> 
  </td>
</tr>
